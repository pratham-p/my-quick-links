{
  "manifest_version": 3,
  "name": "myLinks",
  "version": "1.0",
  "description": "Custom short links like `my google` -> https://google.com",
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "manage.html",
    "default_title": "myLinks Manager"
  },
  "omnibox": { "keyword": "my" }
}
