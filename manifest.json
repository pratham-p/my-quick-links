{
  "manifest_version": 3,
  "name": "My Quick Links",
  "version": "1.4",
  "description": "Create and manage custom short links for quick access to your favorite sites. Example: 'my gmail' -> https://mail.google.com",
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "128": "icons/icon128.png",
    "48": "icons/icon48.png",
    "16": "icons/icon16.png"
  },
  "action": {
    "default_popup": "manage.html",
    "default_title": "My Quick Links"
  },
  "omnibox": { "keyword": "my" }
}
